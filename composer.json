{
  "name": "jecovier/laravel-sandbox-schema",
  "description": "Automatically create your DB as you make api calls",
  "keywords": [
    "jecovier",
    "fractal",
    "api",
    "appstract",
    "laravel",
    "response",
    "macros",
    "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jose Espinoza Conforme",
      "email": "jecovier@gmail.com",
      "homepage": "https://github.com/jecovier",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "laravel/framework": ">=5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Jecovier\\SandboxSchema\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jecovier\\SandboxSchema\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jecovier\\SandboxSchema\\SandboxSchemaServiceProvider"
      ]
    }
  }
}
